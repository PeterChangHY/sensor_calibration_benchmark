<tr id="bag">
<td>
{{bag}}
</td>
<td>
    <table id="chart_row">
        <tr>
        <th>Metric</th> <th>Baseline</h2></th> <th>Target</th>
        </tr>
        <td>
            Confusion Matrix
        </td>
        <td>
            <table id='confusion_matrix'>
                <tr>
                    <td> total#: {{ confusion_matrix['total_number']['baseline'] }}</td> <td>Condition positive</td> <td>Condition negative</td>
                </tr>
                <tr>
                     <td style="border: 1px solid black">Test positive</td>
                     <td style="border: 1px solid black">{{ confusion_matrix['True Positive']['baseline'] }}</td>
                     <td style="border: 1px solid black">{{ confusion_matrix['False Positive']['baseline'] }}</td>
                </tr>
                <tr>
                     <td style="border: 1px solid black">Test negative</td>
                     <td style="border: 1px solid black">{{ confusion_matrix['False Negative']['baseline'] }}</td>
                     <td style="border: 1px solid black">{{ confusion_matrix['True Negative']['baseline'] }}</td>
                </tr>
                </table>
        </td>
        <td>
            <table id='confusion_matrix'>
                <tr>
                    <td>total#: {{ confusion_matrix['total_number']['target'] }}</td> <td>Condition positive</td> <td>Condition negative</td>
                </tr>
                <tr>
                     <td style="border: 1px solid black">Test positive</td> 
                     <td style="border: 1px solid black">{{ confusion_matrix['True Positive']['target'] }}</td> 
                     <td style="border: 1px solid black">{{ confusion_matrix['False Positive']['target'] }}</td>
                </tr>
                <tr>
                     <td style="border: 1px solid black">Test negative</td> 
                     <td style="border: 1px solid black">{{ confusion_matrix['False Negative']['target'] }}</td>
                     <td style="border: 1px solid black">{{ confusion_matrix['True Negative']['target'] }}</td>
                </tr>
                </table>
        </td>

        {% for row in ratios %}
        <tr>
            <td class=black> {{ row['name'] }} </td>
            <td> {{ row['baseline'] }} </td>
            <td> {{ row['target'] }} </td>
        </tr>
        {% endfor %}
    </table>
</td>
{% if chart_path is not none %}
<td><img width=960 height=540 src="{{ chart_path | escape }}"></img></td>
{%endif %}
</tr>
